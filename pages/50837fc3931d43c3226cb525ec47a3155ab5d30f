    <script type="text/javascript">
    	
        function resize() {
            $("#hbidprice img").width($(window).width());
            $("#hbidprice img").height($(window).width() * .67);

            if ($("#hbidprice img").height() <= $(window).height()) {
                $("#hbidprice img").height($(window).height());
                $("#hbidprice img").width($(window).height() * 1.5);
            }
        }
        $(document).ready(function () { 
					resize();
				});
				$(window).load(function() {
					$(".dynamic-img").each(function() {
						var theImage = new Image();
						theImage.src = $(this).attr("src");		
						
						if ( typeof theImage.width != "undefined" && theImage.width > 32) {
							$(this).parents(".automated-graph").show();			
						}
					});
				});
        $(window).resize(function () { resize(); });
		
</script>
    
